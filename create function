createLT()          --生成連跳
createMaxHp()       --生成最大生命值
createHp()          --生成當前生命值
createjumpheight()  --生成跳躍高度
createCrawlSpeed()  --生成潛行速度
createWalkSpeed()   --生成走路速度
createRunSpeed()    --生成奔跑速度
createSwimSpeed()   --生成游泳速度
